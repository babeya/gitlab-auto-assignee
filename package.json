{
  "name": "gitlab-auto-assignee",
  "version": "1.0.0",
  "description": "Auto asign gitlab mr",
  "main": "index.js",
  "repository": "https://github.com/babeya/gitlab-auto-assignee.git",
  "author": "Arthur BABEY <git@ababey.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "init-config": "cp ./config.ts.dist config.ts; cp ./rules.ts.dist rules.ts",
    "watch": "concurrently --kill-others yarn:watch-*",
    "watch-typescript": "tsc -w",
    "watch-node": "nodemon dist/src/index.js",
    "start": "node dist/src/index.js",
    "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:ci": "npx jest --ci --runInBand --silent --colors --coverage",
    "check-prettier": "prettier ./src -c"
  },
  "dependencies": {
    "@types/node": "^14.6.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pkg": "^4.4.9",
    "request": "^2.88.2",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "concurrently": "^5.3.0",
    "jest": "^26.6.0",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1"
  }
}
